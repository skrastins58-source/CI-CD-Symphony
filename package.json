{
  "name": "ci-cd-symphony",
  "version": "1.0.0",
  "description": "Pilnībā automatizēta CI/CD plūsma ar delta analīzi, statusa badge, dokumentācijas ģenerēšanu, Slack notifikācijām, drošības skenēšanu un baseline atjaunošanu.",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lighthouse": "lhci autorun",
    "analyze": "webpack-bundle-analyzer dist/main.js",
    "ci:analysis": "node scripts/run-analysis.js",
    "ci:badges": "node scripts/generate-badges.js",
    "ci:comment": "node scripts/comment-pr.js",
    "ci:slack": "node scripts/slack-notify.js",
    "ci:baseline": "node scripts/update-baseline.js"
  },
  "keywords": [
    "ci",
    "cd",
    "lighthouse",
    "coverage",
    "bundle-size",
    "automation"
  ],
  "author": "skrastins58-source",
  "license": "MIT",
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "jest": "^29.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-bundle-analyzer": "^4.0.0",
    "html-webpack-plugin": "^5.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^9.0.0",
    "style-loader": "^3.0.0",
    "css-loader": "^6.0.0",
    "serve": "^14.0.0"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "express": "^4.18.0"
  }
}